<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    
    {% block stylesheets %}
        {% stylesheets filter='?uglifycss' filter='cssrewrite'
        'vendors/bootstrap/css/bootstrap.min.css'
        '@AdminBundle/Resources/public/vendors/dist/css/AdminLTE.min.css'
        '@AdminBundle/Resources/public/vendors/dist/css/skins/_all-skins.min.css'
        '@AdminBundle/Resources/public/vendors/plugins/iCheck/flat/blue.css'
        '@AdminBundle/Resources/public/vendors/plugins/jvectormap/jquery-jvectormap-1.2.2.css'
        '@AdminBundle/Resources/public/vendors/plugins/datepicker/datepicker3.css'
        '@AdminBundle/Resources/public/vendors/plugins/daterangepicker/daterangepicker-bs3.css'
        '@AdminBundle/Resources/public/css/custom.css'

        output='admin-panel/compiled/app.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}

  </head>
  <body class="hold-transition skin-blue layout-boxed sidebar-mini">
    <div class="wrapper">
        <!-- Header -->
        <header class="main-header">
            {{ include('AdminBundle:Basic:header.html.twig')}}
        </header>
      
        <!-- Main Sidebar -->
        <aside class="main-sidebar">
            {{ include('AdminBundle:Basic:main_sidebar.html.twig')}}
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            {% block content %} {% endblock %}
        </div>
      
        <footer class="main-footer">
            {{ include('AdminBundle:Basic:footer.html.twig')}}
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            {{ include('AdminBundle:Basic:control_sidebar.html.twig')}}
        </aside>
        
        <!-- Add the sidebar's background. This div must be placed
           immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->

     {% block javascript %}
        <script>
            AppAdmin = {
                Routing: {
                    user_search: "{{ path('admin-users-search', {type: '#'})}}",
                    user_profile: "{{ path('admin-users-profile', {id: '#'})}}",
                    user_block: "{{ path('admin-users-block', {id: '#'})}}",
                    get_author_compositions: "{{ path('admin-author-compositions', {id: '#'})}}",
                    show_author_composition: "{{ path('admin-composition-show', {id: '#'})}}",
                    composition_block: "{{ path('admin-composition-block', {id: '#'})}}",
                    composition_search: "{{ path('admin-composition-search', {type: '#'})}}"
                },
                Data: {}        
            };
        </script>
        {% javascripts filter='?uglifyjs2'
        '@AdminBundle/Resources/public/vendors/plugins/jQuery/jQuery-2.1.4.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/jQuery/jquery-ui.min.js'
        '@AdminBundle/Resources/public/vendors/bootstrap/js/bootstrap.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/raphael/raphael.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/sparkline/jquery.sparkline.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
        '@AdminBundle/Resources/public/vendors/plugins/knob/jquery.knob.js'
        '@AdminBundle/Resources/public/vendors/plugins/moment/moment.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/daterangepicker/daterangepicker.js'
        '@AdminBundle/Resources/public/vendors/plugins/datepicker/bootstrap-datepicker.js'
        '@AdminBundle/Resources/public/vendors/plugins/slimScroll/jquery.slimscroll.min.js'
        '@AdminBundle/Resources/public/vendors/plugins/fastclick/fastclick.min.js'
        '@AdminBundle/Resources/public/vendors/dist/js/app.min.js'
       
        output='admin-panel/compiled/app.js' %}
      
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>
    {% endblock %}
  </body>
</html>
